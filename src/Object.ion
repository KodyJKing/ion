const ion = import './'

export class Object extends null
    constructor: (properties) ->
        if properties?
            for key, value of properties
                @[key] = value
    toString: -> @name
    properties:
        toJSON: ->
            const properties = {}
            if @@id?
                properties.$ = @@id
            for key, value of @ if @hasOwnProperty(key)
                properties[key] = value
            return properties
